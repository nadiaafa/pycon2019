<odoo>
    <data noupdate="0">
        <!-- Rooms -->
        <record id="pycon_room1" model="pycon.room">
            <field name="name">Room1</field>
            <field name="capacity">30</field>
        </record>
        <record id="pycon_room2" model="pycon.room">
            <field name="name">Room2</field>
            <field name="capacity">40</field>
        </record>
        <record id="pycon_room3" model="pycon.room">
            <field name="name">Room3</field>
            <field name="capacity">30</field>
        </record>
        <record id="pycon_room4" model="pycon.room">
            <field name="name">Room4</field>
            <field name="capacity">38</field>
        </record>

        <!-- Speakers -->
        <record id="pycon_speaker1" model="pycon.speaker">
            <field name="name">Speaker1</field>
        </record>
        <record id="pycon_speaker2" model="pycon.speaker">
            <field name="name">Speaker2</field>
        </record>
        <record id="pycon_speaker3" model="pycon.speaker">
            <field name="name">Speaker3</field>
        </record>
        <record id="pycon_speaker4" model="pycon.speaker">
            <field name="name">Speaker4</field>
        </record>

        <!-- Presentations -->
        <record id="pycon_presentation1" model="pycon.presentation">
            <field name="name">Getting started with Odoo</field>
            <field name="date" eval="time.strftime('%Y-%m-%d')"/>
            <field name="room" ref="pycon.pycon_room1"/>
            <field name="speakers" eval="[(4, ref('pycon.pycon_speaker1')), (4, ref('pycon.pycon_speaker2')), (4, ref('pycon.pycon_speaker3'))]"/>
        </record>
        <record id="pycon_presentation2" model="pycon.presentation">
            <field name="name">ElasticSearch presentation</field>
            <field name="date" eval="time.strftime('%Y-%m-%d')"/>
            <field name="room" ref="pycon.pycon_room1"/>
            <field name="speakers" eval="[ (4, ref('pycon.pycon_speaker4'))]"/>
        </record>
        <record id="pycon_presentation3" model="pycon.presentation">
            <field name="name">TDD</field>
            <field name="date" eval="time.strftime('%Y-%m-%d')"/>
            <field name="room" ref="pycon.pycon_room1"/>
            <field name="speakers" eval="[ (4, ref('pycon.pycon_speaker1')),(4, ref('pycon.pycon_speaker4'))]"/>
        </record>
    </data>
</odoo>